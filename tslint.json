{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }],
        "quotemark": [true, "single"],
        "object-literal-sort-keys": false,
        "arrow-parens": [true, "ban-single-arg-parens"],
        "interface-name": false
    },
    "rulesDirectory": []
}